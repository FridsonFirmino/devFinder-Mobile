{
  "expo": {
    "name": "devFinder",
    "slug": "devFinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Oval.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141D2F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#141D2F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
